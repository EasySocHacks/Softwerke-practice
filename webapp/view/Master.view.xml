<mvc:View
    controllerName="itmo2021calendareny.controller.Master"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns:core="sap.ui.core"
    displayBlock="true"
    xmlns="sap.m"
>
	<Shell id="shell">
		<App id="app">
			<pages>
				<Page id="page" title="{i18n>title}">
                    <content>
                        <core:Fragment 
                            fragmentName="itmo2021calendareny.view.Calendar"
                            type="XML"
                        />
					</content>
				</Page>
			</pages>
		</App>
	</Shell>
</mvc:View>
